<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.haon.pay.admin.PayAdminDAO">
   <select id="payList" resultType="kr.co.haon.pay.PayVO">
      select pay_id, room_name, user_id, pay_static, pay_price from haon_pay
   </select>
   <delete id="delete">
      delete from haon_pay where pay_id=#{pay_id}
   </delete>
</mapper>