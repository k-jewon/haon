<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.haon.index.admin.IndexAdminDAO">
   <select id="bookCount" resultType="kr.co.haon.book.BookVO">
      SELECT COUNT(book_num) FROM haon_book;
   </select>
   <select id="member" resultType="kr.co.haon.user.UserVO">
      SELECT COUNT(user_id) FROM haon_user;
   </select>
</mapper>